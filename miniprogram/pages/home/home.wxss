/* miniprogram/pages/home/home.wxss */
.home_bg_image{
  width: 100%;
}
.home_title {
  transform: translateY(-55%);
}
.home_link_out_container{
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: -180rpx 0 30rpx;
}
.home_link_container{
  width: 94%;
  /* margin: -180rpx auto 30rpx; */
  background-color: #fff;
  box-sizing: border-box;
  padding: 15rpx 20rpx;
  border-radius: 20rpx;
}
.home_link_title_container>text{
  font-size: 0.9rem;
  margin-left:8rpx;
  margin-bottom: 15rpx;
  vertical-align: middle;
  font-weight: bold;
}
.home_link_title_container{
  margin-bottom: 20rpx;
}
.home_link_list_container{
  display: flex;
  flex-wrap: wrap;
  width: 100%;
  justify-content: space-between;
}
.home_link_list_item{
  width: 49%;
}